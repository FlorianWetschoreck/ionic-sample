.byline,.main_menu .fa {
  -webkit-font-smoothing:antialiased
 }

 .event .media-body a.title {
  font-weight:700
 }
 .x_content {
  // padding:0 5px 6px;
  float:left;
  clear:both;
  margin-top:5px
 }

 .widget {
  min-width:250px;
  max-width:310px
 }
 .timeline.widget {
  min-width:0;
  max-width:inherit
 }

 .byline,.view p {
  font-style:italic
 }

 .timeline .byline {
  padding:.25em 0
 }
 .byline {
  font-size:.9375em;
  line-height:1.3;
  color:#aab6aa
 }

 ul.timeline li {
  position:relative;
  border-bottom:1px solid #e8e8e8;
  clear:both;
  list-style-type:none;
 }


